{
  "name": "xeta",
  "version": "0.2.2",
  "description": "lightweight http client for web and node",
  "repository": "https://github.com/nivrith/xeta",
  "author": "Nivrith <nivrithgomatam@gmail.com>",
  "license": "MIT",
  "keywords": [
    "xeta"
  ],
  "scripts": {
    "serve": "npx webpack-dev-server --env.mode=development",
    "serve:prod": "npx webpack-dev-server --env.mode=production",
    "compile": "npm run clean && npx tsc ",
    "test": "nyc mocha --require ts-node/register test/**/*.spec.ts",
    "prepublishOnly": "npm run compile",
    "pretest": "npm run clean && npm run compile",
    "clean": "del-cli lib",
    "release": "npx np",
    "docs": "typedoc src",
    "devtools": "node --inspect --inspect-brk",
    "benchmark": "tsc -b && node",
    "version": "npx auto-changelog -p && git add CHANGELOG.md"
  },
  "files": [
    "lib/**/*"
  ],
  "types": "lib/**/*.d.ts",
  "main": "lib/index.js",
  "browser": {
    "lib/index.js": "lib/browser.js"
  },
  "dependencies": {
    "url": "^0.11.0"
  },
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/benchmark": "^1.0.31",
    "@types/chai": "^4.2.1",
    "@types/cli-table2": "^0.2.2",
    "@types/mocha": "^5.2.7",
    "auto-changelog": "^1.16.1",
    "awesome-typescript-loader": "^5.2.1",
    "benchmark": "^2.1.4",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "cli-table2": "^0.2.0",
    "css-loader": "^3.2.0",
    "del-cli": "^2.0.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.3.0",
    "fast-memoize": "^2.5.1",
    "html-webpack-plugin": "^3.2.0",
    "logdown": "^3.2.8",
    "microtime": "^3.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "ora": "^3.4.0",
    "style-loader": "^1.0.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  }
}
